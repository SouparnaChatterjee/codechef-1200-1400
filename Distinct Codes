#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    string s;
    cin>>t;
    while(t--)
    {
        cin>>s;
        set<string> s2;
        int n=s.size();
        for(int i=0;i<n-1;i++)
        {
            string s1=s.substr(i,2);
            s2.insert(s1);
        }
        cout<<s2.size()<<endl;
    }
}
