#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,n;
    
    cin>>t;
    while(t--)
    {
        cin>>n;
        unordered_set<int> sett;
        int count=0;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            sett.insert(a[i]);
        }
        cout<<sett.size()<<endl;;
    }
}
